package at.fhooe.mc.linzbrowser.android;

public class DataHotspot {

	public int latitude;
	public int longitude;
	public String name;
	public String shortText;
	public int yearStart;
	public int yearEnd;
	public String city;
	public int zip;
	public String street;
	public String homepage;

	public DataHotspot(int latitude, int longitude, String name, String shortText, int yearStart, int yearEnd, String city, int zip, String street, String homepage){
		this.latitude = latitude;
		this.longitude = longitude;
		this.name = name;
		this.shortText = shortText;
		this.yearStart = yearStart;
		this.yearEnd = yearEnd;
		this.city = city;
		this.zip = zip;
		this.street = street;
		this.homepage = homepage;
	}
	
}
